{
    "version": "3.1.3-beta2",
    "description": "Extend your GitHub workflow beyond your browser.",
    "homepage": "https://desktop.github.com/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://central.github.com/deployments/desktop/desktop/latest/GitHubDesktop-3.1.3-beta2-x64-full.nupkg?env=beta",
